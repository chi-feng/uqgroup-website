RewriteEngine On
# RewriteBase /~feng/uqgroup/
RewriteBase /uqgroup/

RewriteRule ^logout?$ admin.php?logout [NC,L]
RewriteRule ^admin?$ admin.php [NC,L]
RewriteRule ^edit-article-([0-9.-]+)?$ admin.php?edit_article&order=$1 [NC,L]

RewriteRule ^/?$ index.php?p=home [NC,L]
RewriteRule ^([A-Za-z0-9-]+)?$ index.php?p=$1 [NC,L]
